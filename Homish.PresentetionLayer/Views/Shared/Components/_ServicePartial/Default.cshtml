@model List<Homish.EntityLayer.Concrete.Service>
<section class="w3l-features14">
    <div class="w3l-feature-6-main py-5">
        <div class="container py-lg-5">
            <div class="wrapper-max">
                <div class="header-section text-center mb-5">
                    <h6 class="sub-title">Sağladığımız Hizmetler</h6>
                    <h3 class="hny-title two">
                        Sunduğumuz Hizmetler
                    </h3>

                </div>
                <div class="grid mt-lg-4">
                    @foreach (var item in Model)
                    {
                         <div class="w3l-feature-6-gd">
                        <div class="icon"><span class="@item.ServiceIcon"></span></div>
                        <div class="w3l-feature-6-gd-info">
                            <h3><a href="#url">@item.ServiceTitle</a></h3>
                            <p class="pr-lg-5">
                               @item.ServiceDescription
                            </p>
                        </div>
                    </div>
                    }
                   
                    
                </div>

            </div>
        </div>
    </div>
</section>